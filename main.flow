import string;
import grammar;
import parsing;
import validation;

main() {
    p = parse("
        VAR x : [[INT]];
         x:= UPD(x, 0, 1);
    ");


    // UPD([1, 2, 3], 0, 1/0);

    if (p != flow("PARSING FAILED")) {
        validateMessage = validate(p);
        println(validateMessage);
    }

    quit(0);
}
